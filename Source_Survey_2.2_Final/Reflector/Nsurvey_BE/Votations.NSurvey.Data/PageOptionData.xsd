<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="PageOptionData" targetNamespace="http://tempuri.org/PageOptionData.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/PageOptionData.xsd" xmlns:mstns="http://tempuri.org/PageOptionData.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
